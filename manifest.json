{
    "name": "Scele Assigment To Google Calendar",
    "version": "1.0",
    "description": "Automatically add assigment to Google calendar",
    "icons": { "16": "newtab_search16.png", "48": "newtab_search48.png", "128": "newtab_search128.png" },
    "permissions": ["tabs", "contextMenus", "scripting"],
    "host_permissions": [
        "https://scele.cs.ui.ac.id/",
        "*://*/*"
      ],
    "background": {
        "service_worker": "background.js"
      },
    "action": {"default_icon": "newtab_search16.png"},
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js":      ["content.js"]
        
    }],
    "manifest_version": 3
  }